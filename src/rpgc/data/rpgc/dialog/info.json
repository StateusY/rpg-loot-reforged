{
  "type": "minecraft:confirmation",
  "title": "RPGC Info",
  "external_title": "Info",
  "body": [
    {
      "type": "minecraft:plain_message",
      "contents": {
        "text": "Thank you for using RPGC Reforged!"
      }
    },
    {
      "type": "minecraft:plain_message",
      "contents": "This version is in very early development, so expect issues.",
      "width": 250
    },
    {
      "type": "minecraft:plain_message",
      "contents": [
        {
          "text": "Please report issues on "
        },
        {
          "text": "Github",
          "color": "#9999ff",
          "underlined": true,
          "click_event": {
            "action": "open_url",
            "url": "https://github.com/StateusY/rpg-loot-reforged/issues"
          },
          "hover_event": {
            "action": "show_text",
            "value": "https://github.com/StateusY/rpg-loot-reforged/issues"
          }
        },
        "!"
      ]
    },
    {
      "type": "minecraft:plain_message",
      "contents": [
        {
          "text": "RPGC Reforged v1.0\nConfig version 1\n",
          "color": "gray"
        },
        {
          "text": "indev",
          "color": "gold"
        }
      ]
    }
  ],
  "can_close_with_escape": true,
  "pause": true,
  "after_action": "close",
  "yes": {
    "label": "Config",
    "action": {
      "type": "minecraft:run_command",
      "command": "function rpgc:config"
    }
  },
  "no": {
    "label": "Back"
  }
}

